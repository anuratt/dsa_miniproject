             <item><property name="text"><string>üü† High</string></property></item>
             <item><property name="text"><string>üü° Medium</string></property></item>
             <item><property name="text"><string>üü¢ Low</string></property></item>
            </widget></item>
           <item><widget class="QPushButton" name="btnAddToQueue"><property name="text"><string>‚ûï  Add to Queue</string></property></widget></item>
           <item><widget class="QPushButton" name="btnAdmitNext"><property name="text"><string>‚úÖ  Admit Next Patient</string></property></widget></item>
           <item><widget class="QPushButton" name="btnClearQueue"><property name="text"><string>üóë  Clear Queue</string></property></widget></item>
