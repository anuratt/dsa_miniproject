            </widget></item>
           </layout>
          </widget></item>

          <item><widget class="QGroupBox" name="gbCritical">
           <property name="title"><string>Critical Cases</string></property>
           <layout class="QVBoxLayout">
            <item><widget class="QLabel" name="lblCritical">
             <property name="text"><string>0</string></property>
             <property name="alignment"><set>Qt::AlignCenter</set></property>
             <property name="styleSheet"><string>font-size:38px; font-weight:bold; color:#C53030;</string></property>
            </widget></item>
           </layout>
          </widget></item>

