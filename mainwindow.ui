       <attribute name="title"><string>Triage Queue</string></attribute>
       <layout class="QHBoxLayout">
        <property name="margin"><number>16</number></property>

        <!-- Add to Queue Form -->
        <item>
         <widget class="QGroupBox" name="gbAddQueue">
          <property name="title"><string>Add Patient to Triage Queue</string></property>
          <property name="maximumWidth"><number>320</number></property>
