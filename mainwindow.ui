    <item>
     <widget class="QFrame" name="headerFrame">
      <property name="minimumHeight"><number>64</number></property>
      <property name="styleSheet"><string>background:#2B6CB0; color:white;</string></property>
      <layout class="QHBoxLayout">
       <property name="leftMargin"><number>20</number></property>
       <property name="rightMargin"><number>20</number></property>
       <item>
