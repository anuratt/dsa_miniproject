            <widget class="QLineEdit" name="queueName"><property name="placeholderText"><string>Enter patient name</string></property></widget></item>
           <item><property name="label"><string>Age:</string></property>
            <widget class="QSpinBox" name="queueAge"><property name="minimum"><number>1</number></property><property name="maximum"><number>120</number></property></widget></item>
           <item><property name="label"><string>Gender:</string></property>
            <widget class="QComboBox" name="queueGender">
             <item><property name="text"><string>Male</string></property></item>
             <item><property name="text"><string>Female</string></property></item>
             <item><property name="text"><string>Other</string></property></item>
            </widget></item>
           <item><property name="label"><string>Disease/Complaint:</string></property>
            <widget class="QLineEdit" name="queueDisease"><property name="placeholderText"><string>Primary complaint</string></property></widget></item>
           <item><property name="label"><string>Assign Doctor:</string></property>
            <widget class="QComboBox" name="queueDoctor">
             <item><property name="text"><string>Dr. Sarah Ahmed</string></property></item>
             <item><property name="text"><string>Dr. Rahul Khan</string></property></item>
             <item><property name="text"><string>Dr. Aisha Malik</string></property></item>
             <item><property name="text"><string>Dr. James Cooper</string></property></item>
            </widget></item>
           <item><property name="label"><string>Severity:</string></property>
